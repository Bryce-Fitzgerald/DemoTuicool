<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tuicool.SettinsActivity" >

    <include
        android:id="@+id/tuicool_toolbar"
        layout="@layout/toolbar" />

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tuicool_toolbar"
        android:background="?attr/src_other_activity_bg"
        android:padding="6dp" >

        <RelativeLayout
            android:id="@+id/layout1_clear_img_cache"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_marginTop="8dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:layout_toRightOf="@+id/imageView3"
                android:text="清除图片缓存"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/tv_cache_data"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView2"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@+id/textView2"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout2_share_setting"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout1_clear_img_cache"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="分享设置"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout3_settings_more"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout2_share_setting"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="更多设置"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout4_feed_back"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout3_settings_more"
            android:layout_marginTop="10dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="意见反馈"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout5_update_log"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout4_feed_back"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="更新日志"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout6_check_for_updates"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout5_update_log"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="检查更新"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout7_qiu_rentong"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout6_check_for_updates"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="求认同感"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout8_share_friends"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout7_qiu_rentong"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="分享给好友"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layout9_about_us"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/layout8_share_friends"
            android:layout_marginTop="3dp"
            android:background="?attr/src_LinearLayout_bg" >

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="12dp"
                android:text="关于我们"
                android:textColor="?attr/text_article_title"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="14dp"
                android:src="@drawable/pointer" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>